/* custom select styles */

.jcf-select {
  position: relative;
  border: 1px solid $grey;
  margin: 26px 0;
  min-width: 150px;
  display: block;
  max-width: 416px;
  height: 48px;
}

.jcf-select select {
  z-index: 1;
  left: 0;
  top: 0;
}

.jcf-select .jcf-select-text {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  cursor: default;
  display: block;
  font-family: $font-stack-header;
  font-weight: 500;
  font-size: 12px;
  line-height: 45px;
  text-transform: uppercase;
  letter-spacing: .8px;
  margin: 0 35px 0 15px;
  color: $black;
  &:focus {
    margin: 0 35px 0 14px;
  }
}

.jcf-select .jcf-select-opener {
  position: absolute;
  text-align: center;
  width: 29px;
  bottom: 0;
  right: 0;
  top: 0;

  svg {
    position: absolute;
    top: 50%;
    left: 0;
    transform: translateY(-50%);
  }
}

.jcf-select.jcf-drop-active .jcf-select-opener svg{
  transform: rotate(180deg) translateY(50%);
}

body>.jcf-select-drop {
  position: absolute;
  margin: -1px 0 0;
  z-index: 9999;
}

body>.jcf-select-drop.jcf-drop-flipped {
  margin: 1px 0 0;
}

.jcf-select .jcf-select-drop {
  position: absolute;
  margin-top: 0px;
  z-index: 9999;
  top: 100%;
  left: -1px;
  right: -1px;
}

.jcf-select .jcf-drop-flipped {
  bottom: 100%;
  top: auto;
}

.jcf-select-drop .jcf-select-drop-content {
  border: 1px solid $grey;
}

.jcf-select-drop .jcf-option-hideme {display: none !important;}

/* multiple select styles */

.jcf-list-box {
  overflow: hidden;
  display: inline-block;
  border: 1px solid $grey;
  min-width: 200px;
  margin: 0 15px;
}

/* select options styles */

.jcf-list {
  display: inline-block;
  vertical-align: top;
  position: relative;
  background: #fff;
  line-height: 14px;
  font-size: 12px;
  width: 100%;
}

.jcf-list .jcf-list-content {
  vertical-align: top;
  display: inline-block;
  overflow: auto;
  width: 100%;
}

.jcf-list ul {
  list-style: none;
  padding: 0;
  margin: 0;
  overflow: hidden;
}

.jcf-list ul li {
  overflow: hidden;
  display: block;
  padding: 0;
  border-bottom: 1px solid $grey;

  &:before {
    display: none;
  }

  &:first-child {
    margin-top: -1px;
  }

  &:last-child {
    border: 0;
  }
}

.jcf-list .jcf-overflow {
  overflow: auto;
}

.jcf-list .jcf-option {
  overflow: hidden;
  cursor: default;
  display: block;
  padding: 15px 16px;
  color: $black;
  height: 1%;
  font-family: $font-stack-header;
  font-size: 12px;
  line-height: 20px;
  letter-spacing: 1.5px;
  font-weight: 500;
  text-transform: uppercase;
}

.jcf-list .jcf-disabled {
  color: $cream !important;
}

.jcf-select-drop .jcf-hover,
.jcf-list-box .jcf-selected {
  background: $dark-cream;
}

.jcf-list .jcf-optgroup-caption {
  white-space: nowrap;
  font-weight: bold;
  display: block;
  padding: 5px 9px;
  cursor: default;
  color: #000;
}

.jcf-list .jcf-optgroup .jcf-option {
  padding-left: 30px;
}

/* custom number input styles */

.jcf-number {
  display: inline-block;
  position: relative;
  height: 48px;
  width: 113px;
}

.jcf-number input {
  -moz-appearance: textfield;
  -webkit-appearance: none;
  appearance: none;
}

input[type=number] {
  -moz-appearance: textfield;
}

.jcf-number input::-webkit-inner-spin-button,
.jcf-number input::-webkit-outer-spin-button {
  -webkit-appearance: none;
}

.jcf-number input {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  border: 1px solid $black;
  padding: 3px 32px;
  background: $cream;
  text-align: center;
  margin: 0;
  height: 100%;
}

.jcf-number .jcf-btn-dec,
.jcf-number .jcf-btn-inc {
  position: absolute;
  width: 32px;
  height: 46px;
  top: 1px;
}

.jcf-number .jcf-btn-dec {
  left: 1px;
}

.jcf-number .jcf-btn-inc {
  right: 1px;
}

.jcf-number .jcf-btn-dec:before,
.jcf-number .jcf-btn-inc:before,
.jcf-number .jcf-btn-inc:after {
  position: absolute;
  content: '';
  background: $black;
}

.jcf-number .jcf-btn-dec:before {
  top: 22px;
  left: 18px;
  width: 10px;
  height: 1px;
}

.jcf-number .jcf-btn-inc:before {
  top: 22px;
  left: 4px;
  width: 10px;
  height: 1px;
}

.jcf-number .jcf-btn-inc:after {
  top: 18px;
  left: 9px;
  width: 1px;
  height: 10px;
}

.jcf-number.jcf-disabled .jcf-btn-dec:before,
.jcf-number.jcf-disabled .jcf-btn-inc:before,
.jcf-number .jcf-btn-dec.jcf-disabled:before,
.jcf-number .jcf-btn-inc.jcf-disabled:before {
  opacity: 0.3;
}

.jcf-number.jcf-disabled input {
  background: #ddd;
}

/* common custom form elements styles */

.jcf-disabled {
  opacity: .3;
}

