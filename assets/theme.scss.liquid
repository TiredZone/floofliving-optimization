/*================ Rich Text Editor ================*/
.rte {
  img {
    height: auto;
  }

  table {
    table-layout: fixed;
  }

  ul:not([class]),
  ol:not([class]) {
    margin: 0 0 44px 10px;


    li {
      padding: 0 0 0 14px;
    }

  }
  ol:not([class]) {
    list-style: none;
    counter-reset: newCounter;
    margin: 0 0 44px;
    padding: 0;

    li::before {
      counter-increment: newCounter;
      content: counters(newCounter,".") ". ";
      display: inline-block;
      vertical-align: top;
      margin: 0 6px 0 0;
    }

    li {
      padding: 0 0 0 8px;
      margin-bottom: 12px;
    }
  }
  h2 {
    font-size: 40px;
    line-height: 1.2;
    font-family: $font-stack-body;
    font-weight: 400;
    margin: 0 0 21px;
  }
  h5 {
    font-size: 28px;
    font-weight: 400;
  }
  p + h2 {
    padding-top: 20px;
  }
}

.text-center.rte,
.text-center .rte {
  ul,
  ol {
    margin-left: 0;
    list-style-position: inside;
  }
}

// allow table to scroll for tables in the RTE since we don't know
// how many columns they will contain. Class added by JS.
// sass-lint:disable no-misspelled-properties
.rte-table {
  max-width: 100%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}

// This class is wrapped around YouTube/Vimeo embeds in the RTE
// to make them responsive and prevent layout breaking
.rte__video-wrapper {
  position: relative;
  overflow: hidden;
  max-width: 100%;
  padding-bottom: 56.25%;
  height: 0;
  height: auto;

  iframe {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }
}

// This class is wrapped around tables in the RTE
// to prevent layout breaking with a scrollable parent
.rte__table-wrapper {
  max-width: 100%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}

/*============================================================================
  Responsive tables, defined with .responsive-table on table element.
==============================================================================*/
.responsive-table {
  width: 100%;
}


@include media-query($small) {
  .responsive-table {
    width: 100%;

    thead {
      display: none;
    }

    tr {
      display: block;
    }

    // IE9 table layout fixes
    tr,
    td {
      float: left;
      clear: both;
      width: 100%;
    }

    th,
    td {
      display: block;
      text-align: right;
      padding: $gutter / 2;
      margin: 0;
    }

    td::before {
      content: attr(data-label);
      float: left;
      text-align: center;
      padding-right: 10px;
    }
  }

  // Add a keyline between rows
  .responsive-table-row + .responsive-table-row,
  tfoot > .responsive-table-row:first-child {
    position: relative;
    margin-top: 10px;
    padding-top: $gutter / 2;

  }
}

/*================ Links & Buttons ================*/
.btn ,
div.boost-pfs-filter-load-more .boost-pfs-filter-load-more-button,
.boost-pfs-filter-btn-load-previous-page .js-boost-pfs-filter-btn-load-previous-page {
  @include prefix('user-select', 'none', moz ms webkit spec);
  @include prefix(appearance, none, webkit moz spec);
  display: inline-block;
  width: auto;
  min-width: 280px;
  text-decoration: none;
  vertical-align: middle;
  white-space: nowrap;
  text-align: center;
  border: 1px solid $black;
  background-color: $black;
  color: $cream;
  font-size: 12px;
  line-height: 29px;
  font-weight: 500;
  height: 48px;
  text-transform: uppercase;
  margin: 0;
  padding: 9px 32px;
  letter-spacing: 1.5px;
  transition: all 500ms;
  transition-timing-function: cubic-bezier(0.25, 0.1,  0.25, 1);
  position: relative;

  &:hover {
    background-color: $cream;
    color: $black;
    text-decoration: none;
  }

  &:active {
    background: $black;
    color: $cream;
    opacity: .8;
  }

  &:focus {
    background: $black;
    color: $cream;
    border: 1px solid $grey;


    &:after {
      position: absolute;
      top: -4px;
      left: -4px;
      bottom: -4px;
      right: -4px;
      border: 1px solid $grey;
      content: '';
    }
  }


  &:disabled,
  &.disabled,
  [disabled]{
    border-color: transparent;
    pointer-events: none;
    background-color: rgba($black, 0.6);
  }

  &.btn--arrow {
    text-align: left;
    padding: 5px 135px 5px 23px;
    border-radius: 20px;
    position: relative;
    height: 40px;

    &:after {
      border-radius: 99999px;
    }

    &:before {
      position: absolute;
      top: 50%;
      right: 25px;
      margin-top: -6px;
      content: '';
      width: 30px;
      height: 12px;
      background: url('https://cdn.shopify.com/s/files/1/0573/7018/1784/files/arrow-r-white.svg?v=1625047129');
      background-size: contain;
      transition: opacity 500ms;
      transition-timing-function: cubic-bezier(0.25, 0.1,  0.25, 1);
    }

    &:hover {
      &:before {
        background: url('https://cdn.shopify.com/s/files/1/0573/7018/1784/files/arrow-r.svg?v=1625046840');
      }
    }

    &:focus {
      &:before {
        background: url('https://cdn.shopify.com/s/files/1/0573/7018/1784/files/arrow-r-white.svg?v=1625047129');
      }
    }

  }

  &.btn--border {
    border: 1px solid $black;
    background: $cream;
    color: $black;

    &:hover {
      background-color: $black;
      color: $cream;
      text-decoration: none;
    }

    &:active {
      background: $cream;
      color: $black;
      opacity: .8;
      border-color:$black;
    }

    &:focus {
      background: $cream;
      color: $black;
      border-color:$black;


      &:after {
        position: absolute;
        top: -4px;
        left: -4px;
        bottom: -4px;
        right: -4px;
        border: 1px solid $grey;
        content: '';
      }
    }

    &:disabled,
    &.disabled,
    [disabled]{
      background-color: transparent;
      border-color: rgba($black, .6);
      color: rgba($black, .6);
    }
  }
}


.btn--text {
  display: inline-block;
  vertical-align: top;
  text-decoration: underline;

  &:hover {
    text-decoration: none;
  }
}

