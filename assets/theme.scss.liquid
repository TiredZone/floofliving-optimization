/*================ Slate specific reset ================*/
*,
*::before,
*::after {
  box-sizing: border-box;
}

body,
input,
textarea,
button,
select {
  -webkit-font-smoothing: antialiased;
  -webkit-text-size-adjust: 100%;
}

a:focus {
  color: inherit;
}

img {
  max-width: 100%;
}

/*================ Form element helpers ================*/
form {
  margin: 0;
}

// Prevent zoom on touch devices in active inputs
@include media-query($medium-down) {
  input,
  textarea,
  select {
    font-size: 16px;
  }
}

button,
input[type="submit"],
label[for] {
  cursor: pointer;
}

optgroup {
  font-weight: $font-weight-bold;
}

// Force option color (affects IE only)
option {
  color: $color-body-text;
  background-color: $color-body;
}

// hide outline on focus for elements which are given focus by JS
[tabindex='-1']:focus {
  outline: none;
}

/*============================================================================
  Fast Tap
  enables no-delay taps (FastClick-esque) on supporting browsers
==============================================================================*/
a,
button,
[role="button"],
input,
label,
select,
textarea {
  touch-action: manipulation;
}

/*================ Helper Classes ================*/
.clearfix {
  @include clearfix();
}

.visually-hidden {
  @include visually-hidden();
}

.js-focus-hidden:focus {
  outline: none;
}

.label-hidden {
  @include visually-hidden();

  // No placeholders, so force show labels
  .no-placeholder & {
    @include visually-shown();
  }
}

.visually-shown {
  @include visually-shown();
}

// Only show when JS is not supported
.no-js:not(html) {
  display: none;

  .no-js & {
    display: block;
  }
}

// Only show when JS is supported
.js {
  .no-js & {
    display: none;
  }
}

// Only show when browser cookies are not supported
.supports-no-cookies:not(html) {
  display: none;

  html.supports-no-cookies & {
    display: block;
  }
}

// Only show when browser cookies are supported
.supports-cookies {
  html.supports-no-cookies & {
    display: none;
  }
}

/*============================================================================
  Skip to content button
    - Overrides .visually-hidden when focused
==============================================================================*/
.skip-link:focus {
  @include visually-shown(absolute);
  color: $color-body-text;
  background-color: $color-body;
  padding: $gutter / 2;
  z-index: $z-index-skip-to-content;
  transition: none;
}

/*================ Headings ================*/
body,
input,
textarea,
button,
select {
  font-family: $font-stack-header;
  font-size: 16px;
  line-height: 1.5;
  color: $black;
}

a {
  font-family: $font-stack-header;
  font-size: 12px;
  line-height: 1.667;
  text-decoration: none;
  letter-spacing: 1.5px;
  font-weight: 500;
  color: $black;
  text-transform: uppercase;

  &:hover {
    text-decoration: underline;
  }
}

h1,
h2,
h3,
h4,
h5,
h6 {
  margin: 0;
  font-family: $font-stack-header;
  font-weight: 700;
  line-height: 1.4;
  letter-spacing: 0;
  overflow-wrap: break-word;
  word-wrap: break-word;

  a {
    color: inherit;
    text-decoration: none;
    font-weight: inherit;
  }
}

h1 {
  margin: 0 0 30px;
  font-size: 64px;
  line-height: 1.125;
  text-transform: uppercase;

  @include media-query($small) {
    font-size: 36px;
    line-height: 1.222;
  }
}

h2 {
  margin: 0 0 16px;
  font-family: $font-stack-body;
  font-size: 40px;
  line-height: 1.2;
  font-weight: 400;
  @include media-query($small) {
    font-size: 32px;
    line-height: 1.25;
  }
}

h3 {
  margin: 0 0 24px;
  font-size: 40px;
  line-height: 1.2;
  text-transform: uppercase;

  @include media-query($small) {
    font-size: 32px;
    line-height: 1.25;
  }
}


h4 {
  margin: 0 0 24px;
  font-size: 32px;
  line-height: 1.25;
  text-transform: uppercase;

  @include media-query($small) {
    font-size: 24px;
    line-height: 1.333;
  }
}

h5 {
  margin: 0;
  font-family: $font-stack-body;
  font-size: 36px;
  line-height: 1.286;
  font-size: 400;

  @include media-query($small) {
    font-size: 24px;
    line-height: 1.333;
  }
}

h6 {
  margin: 0;
  font-family: $font-stack-body;
  font-size: 24px;
  line-height: 1.333;
  font-weight: 400;
  @include media-query($small) {
    font-size: 20px;
    line-height: 1.4;
  }
}

.h1 {
  @extend h1;
}

.h2 {
  @extend h2;
}

.h3 {
  @extend h3;
}

.h4 {
  @extend h4;
}

.h5 {
  @extend h5;
}

.h6 {
  @extend h6;
}


/*================ Ul styles ================*/
ul:not([class]) {
  margin: 0 0 24px;
  padding: 0;
  list-style: none;

  li {
    position: relative;
    padding: 0 0 12px 19px;

    &:before {
      position: absolute;
      top: 11px;
      left: 0;
      content: '';
      width: 4px;
      height: 4px;
      background-color: $black;
      border-radius: 50%;
    }
  }
}

/*================ Paragraph styles ================*/
p {
  margin: 0 0 24px;
}

/*================ Blockquote styles ================*/
blockquote {
  position: relative;
  margin: 0;
  font-family: $font-stack-body;
  font-size: 28px;
  line-height: 1.286;
  font-weight: 400;

  p {
    &:before,
    &:after {
      display: inline-block;
      vertical-align: top;
    }

    &:before {
      content: '“';
    }
    &:after {
      content: '”';
    }
  }
}

/*============================================================================
  Grid
    - Based on CSS Wizardry grid
==============================================================================*/

.grid {
  @include clearfix();
  list-style: none;
  margin: 0;
  padding: 0;
  margin-left: -$grid-gutter;
}

.grid__item {
  float: left;
  padding-left: $grid-gutter;
  width: 100%;

  &[class*='--push'] {
    position: relative;
  }
}

/*============================================================================
  Reversed grids allow you to structure your source in the opposite
  order to how your rendered layout will appear.
==============================================================================*/
.grid--rev {
  direction: rtl;
  text-align: left;

  > .grid__item {
    direction: ltr;
    text-align: left;
    float: right;
  }
}

/*============================================================================
  Grid Columns
    - Create width classes, prepended by the breakpoint name.
==============================================================================*/
