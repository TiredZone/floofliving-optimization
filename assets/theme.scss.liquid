.swatch-wrap {
  display: flex;
  flex-direction: column;
}

.swatch.swatch--product {
  margin: 0 0 15px;
  padding: 24px 0 0 90px;
  display: block;
  position: relative;
  border-top: 1px solid rgba($black, .2);

  .header {
    margin: 0 0 9px;
    font-size: 12px;
    line-height: 20px;
    font-family: $font-stack-header;
    text-transform: uppercase;
    position: absolute;
    top: 27px;
    left: 0;
    letter-spacing: .8px;
  }

  input {
    display:none;
  }

  &[data-swatch="Color"] {
    padding-top: 38px;
    border: 0;
    order: -1;

    .header {
      top: 3px;

      em {
        display: inline-block;
        vertical-align: top;
        font-size: 14px;
        font-weight: 500;
        font-style: normal;
        text-transform: uppercase;
        padding: 2px 0 0 34px;
      }
    }
  }

  &[data-swatch="Size"] {
    padding-bottom: 49px;

    label {
      letter-spacing: 3px;
    }
  }

  &.has-request-swatch {
    padding-right: 80px;
  }

  .swatch-element[data-value="Ivory"] label{
    box-shadow: 0 0 0 1px rgba(0,0,0,.2);
  }


  @include media-query($small) {

    &[data-swatch="Size"] {
      padding-bottom: 59px;

      label {
        letter-spacing: 3px;
      }
    }


    &[data-swatch="Color"] {
      padding-top: 38px;
      padding-bottom: 36px;
    }

    &.has-request-swatch {
      padding-right: 0;
    }

    &[data-swatch="Fill"] {
       label {
        text-align: left;
        white-space: normal;
         line-height: 20px;
         padding-top: 3px;
         padding-bottom: 3px;
       }
    }
  }
}

[data-swatch="Fill"] {

  .swatch-element {
    width: 100%;
    margin-right: 0 !important;
  }

  label {
    max-width: calc(100% - 52px);
  }

  @include media-query($small) {
    label {
      max-width: calc(100% - 42px);
    }
  }
}


/* Hide radio buttons.*/
.swatch.swatch--product label {
  float:left;
  min-width:80px !important;
  min-height: 28px !important;
  margin:0;
  border: 1px solid $black;
  background:none;
  font-size: 12px;
  text-align:center;
  line-height: 25px;
  font-weight: 500;
  //white-space:nowrap;
  padding: 0 13px;
  text-transform: uppercase;
  letter-spacing: .8px;
}

.swatch.swatch--product  .color.swatch-element {
  margin: 0 21px 14px 3px;

  label {
    padding: 0;
    min-width: 16px !important;
    height: 16px !important;
    min-height: 16px !important;
    line-height: 0;
    border-radius: 100%;
    border: 0 !important;
    background-size: 125% auto;
    background-position: 50%;
  }

  &:last-child {
    margin-right: 5px;
  }
}

.swatch.swatch--product .swatch-element:not(.color) input:checked + label {
  background:$black;
  color: $cream;
}

.swatch.swatch--product .color input:checked + label {
  box-shadow:  0 0 0 3px white,  0 0 0 4px rgba(26,25,25,.4) !important;
