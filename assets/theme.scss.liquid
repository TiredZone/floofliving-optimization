}

.file>label:after{
  position: absolute;
  content: url('data:image/png;base64,');
  width: 25px;
  height: 25px;
  top: 7px;
  right: 10px;
}

icon-fileload.svg
.radio label>span {
  position: relative;
}
.radio label>span:after {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-65%, -50%) !important;
  content: '';
  background-color: #f6f4ee;
  width: 6px;
  height: 6px;
  border-radius: 50%;
}

input[type="radio"] + label span {
  width: 16px;
  height: 16px;
}

.form_submit_div .btn {
  padding: 14px 30px;
}
.login-page-info {
  margin-top: 20px;
}
.login-page-info  {
  color: #000000;
  letter-spacing: 1.5px;
  text-transform: uppercase;
  font-weight: 700;
}
.login-page-info > a {
  position: relative;
  color: #000000;
  font-weight: 400;
  margin-left: 16px;
}
.login-page-info > a:after {
  position: absolute;
  content: '';
  width: 100%;
  height: 1px;
  background-color: #000;
  bottom: 0px;
  left: 0;
}


.form_container .form-group.file,
.form_container .form-group.image {
  margin-top: 1px;
  margin-bottom: 36px;
}

.formElement_21 {
  margin-bottom: 24px;
}

.formElement_21 .block_label{
  margin: 0 0 13px;
}

.radio-inline {
  padding-left: 3px;
  padding-bottom: 10px;
}

.radio-inline > label{
  font-size: 16px !important;
  letter-spacing: 0;
  padding: 0 0 0 10px !important;
}

.form_container input[type="radio"] + label span {
  margin: -1px 14px 0 0;
}

.login-page-info {
  font-size: 12px;
  line-height: 20px;
  font-weight: 500;
  margin: 31px 0;
  letter-spacing: .8px;
}

.inputfile+label {
  padding-left: 15px;
}


@media only screen and (max-width: 767px) {

  .formElement_3 {
    width: 100% !important;
  }

  .formElement_4,
  .formElement_5 {
    width: 50% !important;
  }

  .form_title_div h2 {
    text-align: left !important;
    font-size: 24px !important;
  }

  .form_title_div h2 span{
    text-align: left;
    font-size: 24px !important;
  }

  .form_description_div {
    padding-bottom: 20px;
    text-align: left !important;
  }

  .form_description_div p {
    text-align: left !important;
  }

  .col-lg-1, .col-lg-10, .col-lg-11, .col-lg-12, .col-lg-2, .col-lg-3, .col-lg-4, .col-lg-5, .col-lg-6, .col-lg-7, .col-lg-8, .col-lg-9, .col-md-1, .col-md-10, .col-md-11, .col-md-12, .col-md-2, .col-md-3, .col-md-4, .col-md-5, .col-md-6, .col-md-7, .col-md-8, .col-md-9, .col-sm-1, .col-sm-10, .col-sm-11, .col-sm-12, .col-sm-2, .col-sm-3, .col-sm-4, .col-sm-5, .col-sm-6, .col-sm-7, .col-sm-8, .col-sm-9, .col-xs-1, .col-xs-10, .col-xs-11, .col-xs-12, .col-xs-2, .col-xs-3, .col-xs-4, .col-xs-5, .col-xs-6, .col-xs-7, .col-xs-8, .col-xs-9 {
    padding-left: 8px;
    padding-right: 8px;
  }

  .form-group.label {
    margin-top: 24px;
    margin-bottom: 6px;
  }

  .form-group.label:first-child {
    margin-bottom: 6px;
  }

  div[class^=formElement] {
    margin-bottom: 6px;
  }

  .form-group.fadeMe.text.label {
    padding-left: 8px;
  }

  .form_container .form-group.file,
  .form_container .form-group.image {
    margin-top: 1px;
    margin-bottom: 26px;
  }

  .form_submit_div {
