.hero__image-wrap-absolute {
  display: none;
}

.first-visit  .first-visit-animate {
  .hero__image-wrap-absolute {
    display: block;
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
    text-align: center;
    background-size: cover;
    background-position: 50%;
    background-repeat: no-repeat;
  }

  .hero {
    .hero__text {
      position: relative;
    }

    .page-width {
      height: 1020px;
      position: static;
    }

    .hero__image-wrap-box {
      height: 1020px;
    }
  }

  @media only screen and (min-width: 1921px) and (min-height: 1441px) {
    .hero__image-wrap-absolute {
      background-position: 50%;
    }
  }

  @media only screen and (max-width: 1441px) {
    .hero__image-wrap-absolute {
      background-position: 23% 50%;
    }

    .hero {
      .page-width {
        height: 725px;
      }

      .hero__image-wrap-box {
        height: 725px;
      }
    }
  }


  @media only screen and (max-width: 1149px) {
    .hero__image-wrap-absolute {
      background-position: 49% 50%;
    }

    .hero {
      .page-width {
        height: 680px;
      }

      .hero__image-wrap-box {
        height: 680px;
      }
    }
  }


  @media only screen and (max-width: 989px) {
    .hero__image-wrap-absolute {
      background-position: 49% 50%;
    }


    .hero {
      .page-width {
        height: 450px;
      }

      .hero__image-wrap-box {
        height: 450px;
      }
    }
  }

  @include media-query($small) {
    .hero__image-wrap-absolute {
      display: block;
      position:static;
      background-size: 100% auto;
      background-position: 0 0;
    }

    .hero {
      .page-width {
        padding: 0;
        height: 900px;
      }

      .hero__image-wrap-box {
        height: auto;
        transform: translateY(-100%);
        position: relative;
        padding: 0 24px;
      }

    }
  }


  @media only screen and (max-width: 509px) {
    .hero:first-child {
      margin-top: 104px;
    }
  }
}

.set-carousel {
  overflow: hidden;
  position: relative;
  background-color: $dark-cream;
  border: 1px solid $black;
  border-width: 1px 0;
  padding-top: 86px;
  padding-bottom: 92px;

  .page-width {
    max-width: 1468px;
  }

  @include media-query($small) {
    padding-top: 70px;
    padding-bottom: 87px;
  }
}


.set-carousel__header {
  max-width: 870px;
  margin: 0 auto 64px;
  padding: 0 15px;

  @include media-query($small) {
    text-align: left !important;
    padding: 0 25px;
    margin-bottom: 40px;
  }
}


.set-carousel__sub_title {
  display: block;
  text-transform: uppercase;
  font-size: 12px;
  line-height: 20px;
  font-family: $font-stack-header;
  letter-spacing: .8px;
  margin: 0 0 17px;

  @include media-query($small) {
    margin-bottom: 9px;
  }
}

.set-carousel__heading {
  margin: 0;

}


.set-carousel__slideshow {
  display: flex;
  flex-wrap: nowrap;
  justify-content: center;

  .slick-dots {
    display: none;
  }


  &:not(.slick-initialized) {
    justify-content: flex-start;

    .set-carousel__slide {
      width: calc(50% - 24px);
    }
  }

  &.slick-initialized {
    .set-carousel__slide {
      width: 680px !important;
    }

    .slick-track {
      display: flex;
    }

    .slick-slide {
      height: auto;
    }
  }

  @include media-query($medium-down) {
    &.slick-initialized {

      .set-carousel__slide {
        width: 620px !important;
      }
    }
  }

  @include media-query($small) {
    &.slick-initialized {

      .set-carousel__slide {
        width: 328px !important;
      }
    }

    .slick-arrow {
      display: none !important;
    }

    .slick-dots {
      top: 105%;
    }


    &:not(.slick-initialized) {
      justify-content: flex-start;
      flex-wrap: wrap;

      .set-carousel__slide {
        width: 100%;
        margin: 0 0 24px;

        &:last-child {
          margin-bottom: 0;
        }
      }
    }
  }
}

.btn-set-carousel {
  top: 50%;
  margin-top: -32px;

  &.btn-prev {
    left: 50%;
    transform: translateX(-416px);
  }

  &.btn-next {
    right: 50%;
    transform: translateX(416px);
  }

  @include media-query($medium-down) {
    &.btn-prev {
      left: 40px;
      transform: translateX(0);
    }

    &.btn-next {
      right: 40px;
      transform: translateX(0);
    }
  }
}


.set-carousel__slide {
  border: 1px solid $grey;
  margin: 0 12px 1px;
  width: calc(100% - 24px);
  text-transform: none;
  position: relative;

  &:hover {
    border: 1px solid $black;
    text-decoration: none;

    .set-carousel__btn {
      color: $cream;
      background-color: $black;
    }
  }

  @include media-query($small) {
    margin: 0 8px 1px;
  }
}

.set-carousel__image {
  display: block;
  padding-top: 50.5%;
  background-size: cover;
  background-position: 50%;

  @include media-query($small) {
    padding-top: 83.5%;
  }
}

.set-carousel__text {
  position: relative;
  padding: 16px 120px 13px 23px;
  margin-bottom: 40px;

  @include media-query($small) {
    padding: 15px 105px 13px 15px;
  }
}

.set-carousel__title {
  margin: 0 0 2px;
  font-size: 14px;
  line-height: 24px;
  font-family: $font-stack-header;
  text-transform: uppercase;
  letter-spacing: .7px;
}

.set-carousel__description {
  font-size: 16px;
  line-height: 24px;
  font-weight: 500;
  font-family: $font-stack-body;
  letter-spacing: .1px;

  @include media-query($medium-up) {
    min-height: 24px;
  }
}

.set-carousel__description p {
  margin: 0;
}

.set-carousel__price {
  position: absolute;
  top: 16px;
  right: 23px;
  font-size: 14px;
  line-height: 24px;
  font-weight: 700;
  letter-spacing: .8px;
  text-align: right;

  @include media-query($small) {
    top: 15px;
    right: 13px;
  }
}

.set-carousel__count {
  font-size: 12px;
  line-height: 20px;
  font-family: $font-stack-header;
  text-transform: uppercase;
  letter-spacing: .8px;
  font-weight: 400;
}

.set-carousel__btn {
  display: block;
  font-size: 14px;
  line-height: 30px;
  padding: 5px;
  height: 40px;
  text-align: center;
  text-transform: uppercase;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  border-top: 1px solid $grey;

  @include media-query($small) {
    font-size: 12px;
    letter-spacing: .8px;
  }
}

