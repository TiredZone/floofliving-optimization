body {
  font-family: "Haas Grot Text Web";
  font-weight: 400;
  line-height: 167% ;
}
.form_title_div h2 {
  font-family: "Haas Grot Text Web";
  line-height: 125% ;
  margin-bottom: 6px;
  letter-spacing: -1.2px;
}
.form_description_div {
  padding-bottom: 36px;
  letter-spacing: 0;
}

.form_description_div p {
  font-family: "Haas Grot Text Web";
  font-weight: 400;
}
.block_label {
  font-weight: 400;
  text-transform: uppercase !important;
  margin: 0 0 1px;
  letter-spacing: 0.8px;
}
.form-group {
  margin-bottom: 16px;
}
.form-group.label:first-child {
  margin-top: 0;
  margin-bottom: 12px;
}
.form-group.label {
  margin-top: 22px;
  margin-bottom: 12px;
}
input.inputfile {
  height: 48px;
}

.form-control {
  height: 48px;
  box-shadow: none;
  font-weight: 500;
  padding: 5px 15px 6px;
}
input.form-control, select.form-control {
  text-transform: uppercase;
  font-weight: 500;
  letter-spacing: .8px;
}

.formElement_3 {
  width: 50% !important;
}

.formElement_4,
.formElement_5 {
  width: 25% !important;
}

.file>label{
  position: relative;
  height: 48px;
  display: flex !important;
  align-items: center;
  width: 100% !important;
  text-transform: uppercase;
  font-weight: 700;
  text-align: left;
}

select {
