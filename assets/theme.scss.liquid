@keyframes fadeIn {
  from {
    opacity: 0;
  }

  to {
    opacity: 1;
  }
}

.hero__image-wrap {
  display: none;

  @include media-query($small) {
    padding-bottom: 128.5%;
  }
}

.hero__image-wrap-static {
  margin: 73px auto 77px;
  transform: scale(2.7);
  width: 258px;

  img {
    margin-left: -30px;
  }

  @media only screen and (max-width: 1441px) {
    transform: scale(2.05);
    margin: 73px auto 77px;

    img {
      margin-left: -7px;
    }
  }

  @media only screen and (max-width: 1249px) {
    transform: scale(1.8);

    img {
      margin: 0;
    }
  }

  @media only screen and (max-width: 1149px) {
    transform: scale(1.4);
    margin: -75px auto 0;
  }

  @media only screen and (max-width: 1023px) {
    transform: scale(1.4);
    margin: -75px auto 0;
  }

  @include media-query($medium-down) {
    transform: scale(1.1);
    margin: -22px auto 0;
  }

  @include media-query($small) {
    transform: scale(1.27);
    margin: 69px auto 77px;
  }
}

.hero__image--has-mobile-image {
  @include media-query($medium-up) {
    .hero__image--mobile {
      display: none !important;
    }
  }

  @include media-query($small) {
    .hero__image--full {
      display: none !important;
    }
  }
}

.img-container {
  position: absolute;
  width: 100vw;
  height: 100vh;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
  z-index: 5;

  @include media-query($small) {
    top: 0;
    transform: translateY(0);
  }
}

.hero__clipped {
  //background-image: url(https://cdn.shopify.com/s/files/1/0573/7018/1784/files/Image.png?v=1625818086);
  background-size: cover;
  background-repeat: no-repeat;
  height: 100vh;
  width: 110vw;
  transform: translateX(-9%);
  -webkit-clip-path: url(#hero__svg);
  clip-path: url(#hero__svg);
}

#hero__svg {
  animation: test5 3s ease 0.5s both;
}

@media only screen and (min-width: 1921px) and (min-height: 1441px) {
  #hero__svg {
    animation: test5 3s ease 0.5s both;
  }
}

@media only screen and (max-width: 1920px) {
  #hero__svg {
    animation: test4 3s ease 0.5s both;
  }
}

@media only screen and (max-width: 1349px) {
  #hero__svg {
    animation: test3 3s ease 0.5s both;
  }
}

@media only screen and (max-width: 1199px) {
  #hero__svg {
    animation: test2 3s ease 0.5s both;
  }
}

@media only screen and (max-width: 1024px) {
  #hero__svg {
    animation: test1 3s ease 0.5s both;
  }
}

@media only screen and (max-width: 889px) {
  #hero__svg {
    animation: test0 3s ease 0.5s both;
  }
}

@include media-query($small) {
  #hero__svg {
    animation: test768 3s ease 0.5s both;
  }
}

@media only screen and (max-width: 400px) and (min-height: 870px) {
  #hero__svg {
    animation: test400 3s ease 0.5s both;
  }
}

@keyframes test400 {
  0% {
    transform: scaleX(15) scaleY(10);
    transform-origin: 10vw 30vh;
  }

  100% {
    transform: scaleX(0.82) scaleY(0.5);
    transform-origin: 78vw 10vh;
  }
}

@keyframes test768 {
  0% {
    transform: scaleX(15) scaleY(10);
    transform-origin: 10vw 30vh;
  }

  100% {
    transform: scaleX(0.8) scaleY(0.6);
    transform-origin: 75vw 8vh;
  }
}

@keyframes test0 {
  0% {
    transform: scaleX(15) scaleY(10);
    transform-origin: 10vw 30vh;
  }

  100% {
    transform: scaleX(0.32) scaleY(0.35);
    transform-origin: 19vw 48vh;
  }
}

@keyframes test1 {
  0% {
    transform: scaleX(15) scaleY(10);
    transform-origin: 10vw 30vh;
  }

  100% {
    transform: scaleX(0.3) scaleY(0.55);
    transform-origin: 19vw 48.3vh;
  }
}

@keyframes test2 {
  0% {
    transform: scaleX(15) scaleY(10);
    transform-origin: 10vw 30vh;
  }

  100% {
    transform: scaleX(0.35) scaleY(0.63);
    transform-origin: 21vw 48.3vh;
  }
}

@keyframes test3 {
  0% {
    transform: scaleX(15) scaleY(10);
    transform-origin: 10vw 30vh;
  }

  100% {
    transform: scaleX(0.35) scaleY(0.73);
    transform-origin: 28vw 48.3vh;
  }
}

@keyframes test4 {
  0% {
    transform: scaleX(10) scaleY(8);
    transform-origin: 10vw 30vh;
  }

  100% {
    transform: scaleX(0.25) scaleY(0.73);
    transform-origin: 37.5vw 48.3vh;
  }
}

@keyframes test5 {
  0% {
    transform: scaleX(10) scaleY(8);
    transform-origin: 10vw 30vh;
  }

  100% {
    transform: scaleX(0.25) scaleY(0.53);
    transform-origin: 37.5vw 48.3vh;
  }
}

