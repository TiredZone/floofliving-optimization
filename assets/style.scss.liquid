.quickview-popup {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 11111;
  display: none;
  &.opened {
    display: block;
  }
  .popup-overlay {
    cursor: pointer;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: #f6f4ee;
    opacity: 0.7;
  }
  .popup-content-wrapper {
    border: 1px solid black;
    padding: 90px 70px 70px;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 90%;
    max-width: 1200px;
    background: #f6f4ee;
    min-height: 50vh;
    max-height: 95vh;
    overflow-y: auto;
    -ms-overflow-style: none; /* IE and Edge */
    scrollbar-width: none; /* Firefox */
    &::-webkit-scrollbar {
      display: none;
    }
  }
  .popup-content {
    height: 100%;
    & > .poodle-animated {
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      width: 90px;
    }
    .tap-to-zoom {
      bottom: 0;
      right: 0;
    }
    .product__info {
      padding-top: 0;
      padding-bottom: 0;
    }
    .product__holder {
      align-items: center;
    }
    .product__images--holder {
      width: 45%;
      position: sticky;
      top: 0;
      left: 0;
      .product__gallery-item {
        font-size: 0;
      }

      .product__images {
        background: transparent;
        min-height: unset;
        .product-gallery__image {
          min-height: unset;
          img {
            position: static !important;
            clip: unset;
            height: 100%;
            width: 100%;
            object-fit: contain;
          }
        }
        .product__gallery-image {
          object-fit: contain;
        }
      }
      .product__gallery .slick-arrow {
        width: 50px;
        height: 50px;
        svg {
          width: 20px;
        }
      }
      .product__gallery .slick-arrow.btn-next {
        right: 6px;
      }
      .product__gallery .slick-arrow.btn-prev {
        left: 6px;
      }
    }
    .product__info--holder {
      width: 55%;
      padding-right: 0;
      display: flex;
      align-items: center;
      height: 100%;
      padding-bottom: 60px;
      .product__info-wrapper {
        align-items: flex-start;
        height: 100%;
        overflow-y: auto;
        -ms-overflow-style: none; /* IE and Edge */
        scrollbar-width: none; /* Firefox */
        &::-webkit-scrollbar {
          display: none;
        }
      }
      .product__head {
        align-items: center;
        .product__title {
          margin-bottom: 0;
        }
        .product__price {
          padding-top: 0;
        }
      }
    }
    .view-product-details {
      text-align: right;
      position: absolute;
      bottom: 0;
      right: 0;
      a {
        text-decoration: underline;
      }
    }
  }
  .popup-close {
    position: absolute;
    top: 10px;
    right: 10px;
    background: transparent;
    border: none;
    padding: 0;
    width: 30px;
    height: 30px;
    display: flex;
    justify-content: center;
    align-items: center;
  }
}
