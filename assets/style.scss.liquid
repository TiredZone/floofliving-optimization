.product__gallery-link {
  cursor: -webkit-zoom-in;
  cursor: zoom-in;
}
.hidden {
  display: none;
}
.cursor.zoom-out,
.cursor.zoom-in {
  background: rgba(0, 0, 0, 0.8);
  display: none;
  padding: 10px;
  position: relative;
  border-radius: 50%;
  &.is-active {
    position: fixed;
    width: 50px;
    height: 50px;
    z-index: 99993;
    display: block;
  }
  &:before,
  &:after {
    content: "";
    display: block;
    background: #f6f4ee;
    position: absolute;
  }
  &:before {
    width: 12px;
    height: 1px;
    top: 50%;
    left: 19px;
    transform: translateY(-50%);
  }
}
.cursor.zoom-in {
  &:after {
    width: 1px;
    height: 12px;
    top: 19px;
    left: 50%;
    transform: translateX(-50%);
  }
}
@keyframes fadeInFadeOut {
  0% {
    opacity: 0;
  }
  30% {
    opacity: 1;
  }
  70% {
    opacity: 1;
  }
  100% {
    opacity: 0;
  }
}
.zoom-instructions {
  width: 150px;
  height: 150px;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 99994;
  opacity: 0;
  pointer-events: none;
  &.animating {
    animation: fadeInFadeOut 4s;
  }
  svg {
    width: 100%;
    height: 100%;
  }
  &:after {
    content: "Click to Zoom";
    position: absolute;
    color: white;
    text-align: center;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    font-family: Haas Grot Text Web, Helvetica, Arial, sans-serif;
    font-weight: 500;
    font-size: 12px;
    line-height: 18px;
    letter-spacing: 1.5px;
    padding: 10px;
    text-transform: uppercase;
  }
}
.tap-to-zoom {
