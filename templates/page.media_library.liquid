<div class="page-width page-width--page">
  <h1 class="text-center">{{ page.title }}</h1>

  <div class="rte">
    {{ page.content }}

    <h2>All Product Images</h2>
{% for product in collections['all'].products %}
  <p><a href="/admin/products/{{product.id}}" target="_blank">{%- render 'product-title', product:product -%}</a></p>
  {% for image in product.images %}
    <p><img src="{{ image.src | product_img_url: "large" }}"></p>
  {% endfor %}
{% endfor %}

  </div>
</div>

